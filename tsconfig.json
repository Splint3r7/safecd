{
  "compilerOptions": {
    "target": "ES2019",
    "module": "CommonJS",
    "rootDir": "src",
    "outDir": "dist",
    "esModuleInterop": true,
    "noImplicitAny": true,
    "strict": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "skipLibCheck": true,
  },
  "exclude": [
    "node_modules",
    "dist",
    "data"
  ]
}